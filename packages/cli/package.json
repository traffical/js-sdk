{
  "name": "@traffical/cli",
  "version": "0.1.0",
  "description": "Traffical CLI - config-as-code for your experimentation platform",
  "type": "module",
  "bin": {
    "traffical": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "yaml": "^2.3.4",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "traffical",
    "feature-flags",
    "experimentation",
    "config-as-code",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traffical/js-sdk"
  },
  "publishConfig": {
    "access": "public"
  }
}

