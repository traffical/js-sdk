{
  "name": "@traffical/core",
  "version": "0.1.2",
  "description": "Pure TypeScript core for Traffical SDK - parameter resolution without I/O",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "release": "bun scripts/release.ts"
  },
  "dependencies": {
    "nanoid": "^5.0.9",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {},
  "keywords": [
    "traffical",
    "feature-flags",
    "experimentation",
    "a/b-testing",
    "parameter-resolution"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traffical/js-sdk"
  },
  "publishConfig": {
    "access": "public"
  }
}
