{
  "name": "@traffical/js-client",
  "version": "0.2.0",
  "description": "Traffical JavaScript SDK for browser environments",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:esm && bun run build:iife",
    "build:esm": "tsc",
    "build:iife": "bun run esbuild.config.ts",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@traffical/core": "workspace:^"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "traffical",
    "feature-flags",
    "experimentation",
    "a/b-testing",
    "javascript",
    "browser"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traffical/js-sdk"
  },
  "publishConfig": {
    "access": "public"
  }
}
