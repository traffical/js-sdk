{
  "name": "@traffical/node",
  "version": "0.1.3",
  "description": "Traffical SDK for Node.js - HTTP client with caching and event tracking",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "release": "bun scripts/release.ts"
  },
  "dependencies": {
    "@traffical/core": "workspace:^"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "traffical",
    "feature-flags",
    "experimentation",
    "a/b-testing",
    "node"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traffical/js-sdk"
  },
  "publishConfig": {
    "access": "public"
  }
}
